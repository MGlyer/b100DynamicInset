{
  "name": "dice",
  "version": "2.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run clean:local && http-server -c-1 -p 0 & npm run watch",
    "watch": "npm run watch:inset & npm run watch:app",
    "watch:inset": "node index.js --watch",
    "watch:app": "webpack --config webpack.dev.js --watch --progress --colors",
    "build:production": "npm run build:inset:production && npm run build:app:production && npm run build:fallback:production",
    "build:inset:production": "node index.js --buildInset --production",
    "build:app:development": "webpack --config webpack.dev.js",
    "build:app:production": "NODE_ENV=production webpack --config webpack.prod.js",
    "build:fallback:production": "node index.js --buildFallback --production",
    "build:article": "gulp buildArticle",
    "deploy": "npm run clean:remote && npm run build:production && node index.js --deploy",
    "clean:local": "rm -rf dist/local/ && mkdir -p dist/local",
    "clean:remote": "rm -rf dist/remote/ && mkdir -p dist/remote"
  },
  "author": "",
  "license": "ISC",
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "auto",
    "htmlWhitespaceSensitivity": "css",
    "jsxBracketSameLine": true,
    "jsxSingleQuote": true,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "dependencies": {
    "chokidar": "^2.0.4",
    "colors": "^1.1.2",
    "dsv-loader": "^2.0.0",
    "handlebars": "^4.1.1",
    "html-minifier": "^4.0.0",
    "node-json-minify": "^1.0.0",
    "prettyjson": "^1.2.1",
    "superagent": "^5.0.2",
    "wsjgraphics-css": "git+https://github.dowjones.net/bentleye/wsjgraphics.css.git",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.2",
    "common-tags": "^1.8.0",
    "css-loader": "^2.1.1",
    "gulp": "^4.0.0",
    "gulp-file-include": "^2.0.1",
    "http-server": "^0.11.1",
    "image-size": "^0.7.3",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "puppeteer": "^1.8.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1",
    "webpack-shell-plugin": "^0.5.0"
  }
}
